{
  "name": "@sahitono-iclient/iserver",
  "version": "0.10.0",
  "description": "",
  "author": "",
  "license": "ISC",
  "keywords": [],
  "exports": {
    "./*": "./dist/*.js",
    "./geometry": "./dist/geometry.mjs",
    "./services": "./dist/services.mjs",
    "./utils": "./dist/utils.mjs",
    "./sm": "./dist/sm.mjs",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ],
      "services/data": [
        "./dist/services/data/*"
      ],
      "services/map": [
        "./dist/services/map/*"
      ],
      "services/network": [
        "./dist/services/network/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "bundle": "esbuild --bundle src/index.ts --outfile=dist/index.bundle.js --format=esm",
    "bundle:browser": "esbuild src/index.ts  --outfile=dist/index.bundle.js --bundle --minify --sourcemap",
    "build": "unbuild",
    "prepublishOnly": "tsc",
    "check": "tsc --noEmit -p ./tsconfig.json"
  },
  "dependencies": {
    "geojson": "^0.5.0",
    "radash": "^12.1.0",
    "ky": "^1.5.0"
  },
  "devDependencies": {
    "@types/geojson": "^7946.0.14"
  }
}
